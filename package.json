{
  "name": "shipengine-js",
  "version": "1.0.0",
  "description": "ShipEngine SDK for node/browser",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "gen": "sh generate-internal-client.sh",
    "tsc:check": "tsc --noEmit",
