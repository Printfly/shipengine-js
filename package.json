{
  "name": "shipengine",
  "version": "1.0.0-alpha.6",
  "description": "ShipEngine SDK for node / browser",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "esm/index.d.ts",
  "files": [
    "cjs",
    "esm"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:esm": "tsc --module esnext --outDir esm",
    "build:cjs": "tsc --module commonjs --outDir cjs",
    "gen:types": "bash ./generate-types.sh && npm run fix",
    "tsc:check": "tsc --noEmit",
    "test": "npm run test:node && npm run test:browser",
    "test:node:watch": "chokidar test/** src/**/** -c 'npm run test:node'",
    "test:browser:watch": "chokidar **/test/** karma.conf.js -c 'npm run test:browser -- --single-run'",
    "test:node": "npm run build:cjs && mocha",
    "test:examples": "ts-node-script -- ./docs/examples-test.ts",
    "test:browser": "npm run build && karma start --single-run",
    "lint": "npm run tsc:check && eslint .",
    "lint:examples": "eslint docs/examples/**/*.ts",
    "sim:update": "git submodule update --recursive --remote",
    "sim": "docker-compose -f simengine/docker-compose.yaml ",
    "sim:start": "npm run sim -- up -d && npm run sim:import",
    "sim:import": "ts-node-script import-rpc.ts",
    "sim:stop": "npm run sim -- down",
    "fix": "npm run lint -- --fix",
    "devdoc": "chokidar typedoc.js src/** -c 'npm run compile:api'",
    "devlit": "chokidar docs/* --ignore '/docs/(api|tutorials|examples).*/' -c 'npm run compile:lit'",
    "fix:examples": "npm run lint:examples -- --fix",
    "compile:md-to-lit": "ts-node-script docs/md-to-lit.ts",
    "compile:api": "typedoc",
    "compile:api:watch": "run-p devdoc serve",
    "compile:lit": "bash scripts/compile-lit.sh && npm run fix:examples",
    "compile:lit:watch": "run-p devlit serve",
    "compile": "npm run compile:api && npm run compile:lit",
    "serve": "live-server ./docs",
    "tag-and-release": "git checkout main && bash ./scripts/release.sh",
    "json2ts": "json2ts"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test:node"
    }
  },
  "engines": {
    "node": "12.x"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@jsdevtools/host-environment": "^2.1.2",
    "@jsdevtools/karma-config": "^3.1.7",
    "@jsdevtools/version-bump-prompt": "^6.1.0",
    "@openapitools/openapi-generator-cli": "^2.0.3",
    "@tsconfig/node12": "^1.0.7",
    "@types/chai": "^4.2.14",
    "@types/karma": "^5.0.1",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.19.3",
    "@types/sinon": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "chai": "^4.2.0",
    "chokidar-cli": "^2.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "fkill": "^7.0.1",
    "husky": "^4.3.6",
    "json-schema-to-typescript": "^10.0.0",
    "karma": "^5.2.3",
    "karma-child-process": "^1.0.4",
    "karma-cli": "^2.0.0",
    "live-server": "^1.2.1",
    "mocha": "^8.2.1",
    "mocha-each": "^2.0.1",
    "mocha-suppress-logs": "^0.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "shipengine-json-schema": "^1.4.0",
    "shipengine-openapi": "^1.4.0",
    "sinon": "^9.2.1",
    "ts-node": "^9.1.1",
    "typedoc": "^0.19.2",
    "typedoc-plugin-internal-external": "^2.2.0",
    "typescript": "^4.0.5",
    "utility-types": "^3.10.0"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "retry-axios": "^2.3.0"
  }
}
